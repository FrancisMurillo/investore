{
  "rules": {
    "import/no-absolute-path": 2,
    "import/newline-after-import": [
      2,
      {
        "count": 1
      }
    ],
    "import/no-cycle": [
      2,
      {
        "maxDepth": 1
      }
    ],
    "import/namespace": 0,
    "import/default": 2,
    "react/prop-types": 0,
    "react/jsx-indent": 0,
    "react/display-name": 0,
    "react/jsx-filename-extension": 0,
    "prettier/prettier": 0,
    "no-console": 2,
    "no-debugger": 2,
    "no-undef": 2
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "prettier/react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jasmine": true
  },
  "globals": {
    "check": true,
    "gen": true,
    "Promise": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    },
    "propWrapperFunctions": [
      "forbidExtraProps"
    ],
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "16.0"
    }
  },
  "plugins": [
    "prettier",
    "module-resolver"
  ],
  "parser": "babel-eslint"
}